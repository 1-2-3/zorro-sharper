{
  "name": "zorro-sharper-doc",
  "version": "1.3.1",
  "description": "针对 ng-zorro-antd 组件库的轻量级增强指令、组件集。",
  "keywords": [
    "angular",
    "angular2",
    "ng2",
    "zorro-sharper",
    "ng-zorro-antd"
  ],
  "author": "1-2-3 <1-2-3>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1-2-3/zorro-sharper/issues"
  },
  "homepage": "https://github.com/1-2-3/zorro-sharper",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port 6200",
    "build": "ng build --configuration production",
    "build-lib": "ng build zorro-sharper",
    "build-doc": "ng build zorro-sharper-doc --configuration production --base-href /zorro-sharper/ && dir && del /S /F /Q docs\\*.* && xcopy /s /e /i /y dist\\zorro-sharper-doc\\*.* docs\\ && copy docs\\index.html docs\\404.html",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "upantd": "tslint --project . --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.0",
    "@angular/common": "^13.2.0",
    "@angular/compiler": "^13.2.0",
    "@angular/core": "^13.2.0",
    "@angular/forms": "^13.2.0",
    "@angular/platform-browser": "^13.2.0",
    "@angular/platform-browser-dynamic": "^13.2.0",
    "@angular/router": "^13.2.0",
    "@delon/abc": "^13.1.0",
    "ng-zorro-antd": "^13.0.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~20.0.1",
    "@angular/cli": "^13.2.0",
    "@angular/compiler-cli": "^13.2.0",
    "@angular/language-service": "^13.2.0",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.14",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "^2.0.1",
    "karma-spec-reporter": "0.0.32",
    "karma-viewport": "^1.0.9",
    "ng-packagr": "^18.2.1",
    "nz-tslint-rules": "^0.901.2",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tsickle": "^0.36.0",
    "tslint": "~6.1.3",
    "typescript": "~4.5.5"
  }
}
